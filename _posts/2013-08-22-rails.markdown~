{% highlight ruby %}
model
class Category <  ActiveRecord::Base
has_many :tags
end

class Tag < ActiveRecord::Base
belongs_to :category
has_many :lists
end

class List < ActiveRecord::Base
belongs_to :tag
has_many :products
end

class Products < ActiveRecord::Base
belongs_to :list
end



{% endhighlight %}

